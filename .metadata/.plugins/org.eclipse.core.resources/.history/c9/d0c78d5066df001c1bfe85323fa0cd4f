package br.com.gft.api.domain;

public class User {
	
	private Integer id;
	private String name;
	private String email;
	private String password;
	
	public User() {
	}

	public User(Integer id, String name, String email, String password) {
		this.id = id;
		this.name = name;
		this.email = email;
		this.password = password;
	}
	
	

}
